<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="1dp"
    android:layout_marginEnd="11dp"
    android:background="@drawable/background_add"
    app:cardCornerRadius="8dp"
    app:strokeColor="@color/secondarycolorshadeandtint1"
    app:strokeWidth="1dp">

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/background_add"
        app:strokeColor="@color/secondarycolorshadeandtint1"
        app:strokeWidth="1dp"/>

    <TextView
        android:id="@+id/textView29"
        android:layout_gravity="top|start"
        android:layout_marginStart="6dp"
        android:layout_marginTop="18dp"
        android:background="@android:color/transparent"
        android:layout_marginBottom="14dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tech Meetup \ncoming soon"
        android:fontFamily="@font/roboto_regular"
        android:textSize="12sp"
        android:textColor="@color/white"/>

    <TextView
        android:layout_gravity="bottom|end"
        android:id="@+id/textView32"
        android:background="@android:color/transparent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="www.techmeetup"
        android:textColor="@color/white"
        android:textSize="5.27sp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="3dp"
        android:fontFamily="@font/roboto_regular"/>
</androidx.cardview.widget.CardView>